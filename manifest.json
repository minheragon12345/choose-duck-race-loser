{
  "manifest_version": 3,
  "name": "Duck Race Manipulator",
  "version": "1.0",
  "description": "Automatically manipulate Duck Race results.",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.online-stopwatch.com/duck-race/"],
      "js": ["content.js", "override.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
